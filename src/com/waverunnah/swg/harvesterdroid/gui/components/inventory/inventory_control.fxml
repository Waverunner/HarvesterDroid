<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Label?>
<fx:root spacing="5.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="inventoryListView" disable="true" VBox.vgrow="ALWAYS">
          <placeholder>
              <Label text="There's no resources in your inventory!" />
          </placeholder>
      </ListView>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <buttons>
          <Button fx:id="removeButton" mnemonicParsing="false" text="Remove" onAction="#removeSelectedResource"/>
            <Button fx:id="addButton" layoutX="135.0" layoutY="18.0" mnemonicParsing="false" text="Add" onAction="#addGalaxyResource"/>
        </buttons>
      </ButtonBar>
   </children>
</fx:root>
